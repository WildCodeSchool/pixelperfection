{% extends 'base.html.twig' %}

{% block title %}Quality{% endblock %}

{% block body %}
    <h1>Quality</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quality.id }}</td>
            </tr>
            <tr>
                <th>Parfume</th>
                <td>{{ quality.perfume }}</td>
            </tr>
            <tr>
                <th>Texture</th>
                <td>{{ quality.texture }}</td>
            </tr>
            <tr>
                <th>Application</th>
                <td>{{ quality.application }}</td>
            </tr>
            <tr>
                <th>Packaging_attractive</th>
                <td>{{ quality.packagingAttractive }}</td>
            </tr>
            <tr>
                <th>Packaging_convenient</th>
                <td>{{ quality.packagingConvenient }}</td>
            </tr>
            <tr>
                <th>Efficiency</th>
                <td>{{ quality.efficiency }}</td>
            </tr>
            <tr>
                <th>Allergy</th>
                <td>{{ quality.allergy ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_quality_index') }}">back to list</a>

    <a href="{{ path('app_quality_edit', {'id': quality.id}) }}">edit</a>

    {{ include('quality/_delete_form.html.twig') }}
{% endblock %}
